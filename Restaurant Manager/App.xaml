<Application x:Class="Restaurant_Manager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Restaurant_Manager" xmlns:sys="http://schemas.microsoft.com/winfx/2009/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/Verification.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/Login.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/GlobalStyles.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
