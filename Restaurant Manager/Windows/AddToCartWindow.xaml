<Window x:Class="Restaurant_Manager.Windows.AddToCartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Restaurant_Manager.Windows" xmlns:controls="clr-namespace:Restaurant_Manager.Classes.Controls"
        mc:Ignorable="d"
        Title="AddToCartWindow" Height="100" Width="130" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <controls:CustomNumbericUpDown Width="100" Height="40" Margin="15,10,15,50" Number="0"></controls:CustomNumbericUpDown>
        <Button Click="Ok_Click" Width="50" Height="20" Style="{StaticResource Shared.SmallButton3}" FontSize="10" Content="Ok" Foreground="White" Margin="70,70,10,10"></Button>
        <Button Click="Cancel_Click" Width="50" Height="20" Style="{StaticResource Shared.SmallButton}" FontSize="10" Content="Cancel" Foreground="White" Margin="10,70,70,10"></Button>
        
    </Grid>
</Window>
